<div class="col-xs-12 col-sm-9">
	<div class="row">
        	<div class="col-sm-9">
        		<div class="container-fluid">
				<div class="panel panel-default">
					<div class="container-fluid">
						<h1><u>Topics</u></h1>
						<ul style="font-size: 16px">
							<% @topics.each do |topic| %>
								<li><strong><%= link_to topic.name, topic_path(topic) %> - 
								<%= topic.description %></strong></li>
							<% end %>
						</ul>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
<br>